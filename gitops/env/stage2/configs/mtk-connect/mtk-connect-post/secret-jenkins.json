{
  "apiVersion": "v1",
  "kind": "Secret",
  "metadata": {
    "name": "jenkins-mtk-connect-apikey",
    "namespace": "jenkins",
    "labels": {
      "jenkins.io/credentials-type": "usernamePassword"
    },
    "annotations": {
      "jenkins.io/credentials-description": "MTK Connect APIKEY"
    }
  },
  "type": "Opaque",
  "stringData": {
    "username": "mtk-connect-admin",
    "password": "##MTKC_APIKEY##"
  }
}
